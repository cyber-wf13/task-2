{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "quote-props": [
            "error",
            "always"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-use-before-define": "off",
        "object-curly-newline": "off",
        "operator-linebreak": "off"
    }
}